{
    "name": "Music Repo",
    "logo": "https://telegra.ph/file/991717f8dacfd35570b0c.jpg",
    "description": "An Advanced Premium Music Bot For Telegram Groups VC.",
    "keywords": ["music", "voice chat", "telegram"],
    "repository": "https://github.com/log-afk-xD/Op-music",
    "stack": "container",
    "env": {
        "API_ID": {
            "description": "Your App ID",
            "required": true
        },
        "API_HASH": {
            "description": "Your App Hash",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "Put Bot Token Of Your Bot",
            "required": true
        },
         "BOT_USERNAME": {
            "description": "Put Bot Username Of Your Bot",
            "required": true
        },
        "STRING_SESSION": {
            "description": "Pyrogram String Session",
            "required": true
        }
    },
    "buildpacks": [
    {
      "url": "heroku/python"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    }
